{
  "name": "phosphor-plugins",
  "version": "0.1.0",
  "description": "A module for building plugin-based applications.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "phosphor-disposable": "^1.0.4"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "expect.js": "^0.3.1",
    "glob-copy": "^0.1.0",
    "istanbul": "^0.3.19",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "steal": "^0.12.1",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build:src": "tsc --project src",
    "build": "npm run build:src",
    "build:example": "mkdirp example/phosphor-plugins && node scripts/copyjs.js",
    "build:test": "tsc --project test/src",
    "prepublish": "npm run build",
    "postinstall": "npm dedupe",
    "test": "mocha test/build/index.js",
    "test:coverage": "istanbul cover --dir test/coverage node_modules/mocha/bin/_mocha -- test/build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phosphorjs/phosphor-plugins.git"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "keywords": [
    "plugin",
    "application"
  ],
  "author": "Steven Silvester <steven.silvester@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phosphorjs/phosphor-plugins/issues"
  },
  "homepage": "https://github.com/phosphorjs/phosphor-plugins"
}
