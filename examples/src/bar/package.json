{
  "private": true,
  "dependencies": {},
  "devDependencies": {},
  "phosphor-plugin": {
    "extensionPoints": [
      {
        "main": "index.js",
        "id": "my-bar:bar-point",
        "factory": "createBarReceiver"
      },
      {
        "main": "index.js",
        "id": "my-bar:css",
        "factory": "createCSSReceiver"
      }
    ],
    "extensions": [
      {
        "main": "index.js",
        "id": "my-bar:bar-ext-0",
        "point": "my-foo:foo-point",
        "factory": "createFooContrib",
        "config": {
          "size": 12,
          "names": ["Mary", "Joe", "Sue"]
        }
      },
      {
        "id": "my-bar:bar-ext-1",
        "point": "my-bar:bar-point",
        "data": "data.json"
      },
      {
        "id": "my-bar:bar-ext-2",
        "point": "my-bar:css",
        "config": { "path": "bar/index.css" }
      }
    ]
  }
}
